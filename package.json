    "test": "vitest",
    "web-norm": "web-norm",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "postinstallmac": "git config core.hooksPath .husky && chmod 700 .husky/*",
    "eslint": "eslint --ignore-pattern '!<relative/path/to/filename>' --cache --max-warnings 10  \"{src,mock}/**/*.{vue,ts,js,tsx}\" --fix  ",
    "commit": "git add . && git-cz",
    "build:demand": "pnpm build:components",
    "build:components": "esno ./scripts/build.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-angular": "^17.0.3",
    "@commitlint/cz-commitlint": "^17.0.3",
    "@iconify-json/ep": "^1.1.8",
    "@iconify-json/ic": "^1.1.9",
    "@types/node": "^18.11.9",
    "@typescript-eslint/parser": "^5.30.7",
    "@unocss/preset-attributify": "^0.46.3",
    "@vitejs/plugin-vue": "3.0.3",
    "@vitejs/plugin-vue-jsx": "2.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/test-utils": "^2.2.1",
    "commitizen": "^4.2.4",
    "cz-customizable": "^6.9.0",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^9.7.0",
    "happy-dom": "^7.6.6",
    "inquirer": "^8.0.0",
    "lint-staged": "^12.4.1",
    "prettier": "^2.7.1",
    "unocss": "^0.46.3",
    "vite": "^3.2.2",
    "vitepress-theme-demoblock": "1.4.2",
    "vitest": "^0.24.5",
    "web-norm": "^1.0.25"
  },
  "dependencies": {
    "esno": "^0.16.3",
    "fs-extra": "^10.1.0",
    "vitepress": "1.0.0-alpha.27",
    "vitepress-demoblock": "^1.0.4",
    "vue": "^3.2.41"
  },
  "lint-staged": {
    "*.{js,ts,vue,jsx,tsx}": [
      "npm run eslint"
    ],
    "*.{js,jsx,ts,tsx,md,html,css,lees,scss,sass}": "prettier --write"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
